{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [
      "./src/"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "Event Cluster",
    "cleverLinks": false,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "linenums": true
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "lenient": true,
    "readme": "./README.md",
    "destination": "./docs/",
    "template": "./node_modules/minami"
  }
}
