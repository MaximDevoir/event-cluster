{
  "name": "event-cluster",
  "version": "0.12.6",
  "description": "A simple event handler with the ability to cluster events",
  "main": "dist/EventHandler.js",
  "scripts": {
    "build:docs": "jsdoc -c .jsdoc.json",
    "build:source": "babel src -d dist",
    "build": "npm run build:source && npm run build:docs",
    "lint": "gulp lint:js",
    "test": "mocha --compilers js:babel-register test/eventhandler.spec.js",
    "codecov": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --recursive --check-leaks --compilers js:babel-register test/eventhandler.spec.js",
    "build-watch": "babel src -w -d dist",
    "postinstall": "node ./scripts/postinstall.js",
    "test-watch": "mocha --watch --compilers js:babel-register test/eventhandler.spec.js"
  },
  "author": "Maxim Devoir",
  "license": "CC0",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-mocha": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "gulp-util": "^3.0.7",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "mocha": "^2.4.5",
    "require-dir": "^0.3.0"
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "dependencies": {
  }
}
